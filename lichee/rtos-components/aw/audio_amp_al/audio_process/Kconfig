menu "audio process"

config COMPONENTS_AW_AUDIO_PROCESS
	bool "Audio Process Support"
	default n
	help
		This is a interface for audio process.

config COMPONENTS_AW_AUDIO_AEC
	bool "Audio AEC Support"
	depends on COMPONENTS_AW_AUDIO_PROCESS
	help
		This is a demo that introduces how to use openmax for aec.

