menu "G2D Devices"

config DRIVERS_G2D
    bool "enable g2d driver"
        default n

config G2D_USE_HWSPINLOCK
    bool "g2d use hwspinlock"
    depends on DRIVERS_G2D
      default n

config HAL_TEST_G2D
    bool "enable g2d hal APIs test command"
	depends on DRIVERS_G2D
        default n

endmenu
